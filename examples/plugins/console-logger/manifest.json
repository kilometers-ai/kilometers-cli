{
  "name": "console-logger",
  "version": "1.0.0",
  "description": "Enhanced console logging for Kilometers CLI",
  "author": "Kilometers.ai",
  "required_tier": "Free",
  "features": [
    "console_logging"
  ],
  "capabilities": [
    "message_handling",
    "error_handling", 
    "stream_events"
  ],
  "security": {
    "signature_algorithm": "SHA256-RSA",
    "public_key_fingerprint": "km_public_key_fingerprint_xyz",
    "embedded_auth": true,
    "runtime_validation": true
  },
  "build_info": {
    "go_version": "1.21+",
    "build_time": "2024-08-01T15:45:00Z",
    "git_commit": "abc123def456",
    "build_host": "secure-build-server",
    "customer_specific": false
  },
  "api_requirements": {
    "minimum_api_version": "1.0",
    "endpoints": [
      "/api/plugins/authenticate",
      "/api/plugins/validate"
    ],
    "permissions": [
      "console_logging"
    ]
  },
  "installation": {
    "target_directories": [
      "~/.km/plugins/",
      "/usr/local/share/km/plugins/",
      "./plugins/"
    ],
    "binary_name": "km-plugin-console-logger",
    "auto_discovery": true
  }
}